<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hengtian.flow.dao.RemindTaskDao">

    <select id="taskRemindList" parameterType="com.hengtian.common.param.TaskQueryParam" resultType="RemindTask">
      SELECT
        *
      FROM
        t_remind_task AS trt LEFT JOIN act_ru_task AS art ON trt.TASK_ID = art.ID_
        LEFT JOIN
    </select>

    <select id="taskRemindedList" parameterType="com.hengtian.common.param.TaskQueryParam" resultType="RemindTask">

    </select>
</mapper>
